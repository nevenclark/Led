#include <FastLED.h>
#define NUM_LEDS 135
#define DATA_PIN 11

CRGB leds[NUM_LEDS];


void setup() {
  FastLED.addLeds<WS2812B, DATA_PIN, GRB>(leds, NUM_LEDS);
}
void loop() {
  for(int i=0;i<NUM_LEDS;i+=5){//this is supposed to make every 5 leds change gold around through each Led then wrap around
    leds[i] = CRGB::Gold;
    leds[(NUM_LEDS-(i+=5))] = CRGB::Silver;//this supposed to make every 5 leds change silver going around through each Led then wrap around
    FastLED.show();
      leds[135,124] = CRGB::Red;//All these are suppose to light up a specific area with the colors that are specified 
       leds[123,104] = CRGB::Teal;
       leds[103,83] = CRGB::Green;
       leds[82,63] = CRGB::Pink;
       leds[62,40] = CRGB::Orange;
       leds[39,21] = CRGB::Red;
       leds[20,0] = CRGB::Blue;
    delay(40);
    leds[i] = CRGB::White;
    leds[(NUM_LEDS-i)] = CRGB::White;
  }
}
